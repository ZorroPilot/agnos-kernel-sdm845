/* disable the SPI */
&qupv3_se5_spi {
	status = "disabled";
};

&qupv3_se2_spi {
	status = "disabled";
};

/* don't use /dev/i2c-4, it's for DP */

/* audio and power monitor(ina230) I2C, /dev/i2c-0 */
&qupv3_se10_i2c {
	status = "ok";

  ti_ina320@64 {
    compatible = "ti,ina230";
    reg = <0x64>;
    /* value is 30mOhm, measured in uohms */
    shunt-resistor = <30000>;
  };

  max98089@10 {
    compatible = "maxim,max98089";
    reg = <0x10>;  /* slave address 0x20 for write, 0x21 for read */
    status = "okay";
  };
};

/* light(RPR-0521RS) and touch panel I2C, /dev/i2c-2 */
&qupv3_se5_i2c {
	status = "ok";

  rpr0521@38 {
    compatible = "rpr0521";
    reg = <0x38>;
  };
};

/* panda SPI */
&qupv3_se0_spi {
	status = "ok";
};


